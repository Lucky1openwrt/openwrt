#include "mt7621_dlink_dir_nand_128m.dtsi"

/ {
	compatible = "dlink,dir-2055-a1", "mediatek,mt7621-soc";
	model = "D-Link DIR-2055 A1";
};

&keys {
	wifi {
		label = "wifi";
		gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
		linux,code = <KEY_RFKILL>;
	};
};

&leds {
	led-1 {
		color = <LED_COLOR_ID_BLUE>;
	};

	led-3 {
		color = <LED_COLOR_ID_BLUE>;
	};
};
